        }
        else
		{
            SendProximityMessage(playerid, 20.0, COLOR_PURPLE, "* %s reaches for their seatbelt, and buckles it up.", GetRPName(playerid));
			SCM(playerid, COLOR_WHITE, "You have put on your seatbelt.");
