 		TextDrawHideForPlayer(playerid, withdrawtd);
		TextDrawHideForPlayer(playerid, deposittd);
		TextDrawHideForPlayer(playerid, transfertd);
		CancelSelectTextDraw(playerid);
	}     	
 	return 1;
}
